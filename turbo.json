{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["tsconfig.json"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", ".vercel/**", "dist/**"]
    },
    "lint": {},
    "format": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "typechain": {},
    "coverage": {},
    "test": {
      "dependsOn": ["^build"]
    },
    "compile": {
      "outputs": ["artifacts", "types", "abi", "bytecode"],
      "inputs": ["contracts/**/*"]
    }
  }
}
