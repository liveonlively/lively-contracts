{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", ".vercel/**", "dist/**", "artifacts"]
    },
    "lint": {},
    "format": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "typechain": {},
    "coverage": {},
    "test": {},
    "compile": {
      "outputs": ["artifacts"]
    }
  }
}
